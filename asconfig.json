{
  "targets": {
    "release": {
      "binaryFile": "build/optimized.wasm",
      "textFile": "build/optimized.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false,
      "enable": ["reference-types"],
      "exportRuntime": true
    }
  },
  "options": {
    "transform": "as-bind"
  }
}